#import Std from "../std"
#import Test from "../test"

Test.test "map.set", function ()
    obj = {}
    obj.set("a", 1)
    Test.expect(obj.a).toBe(1)
    obj.set(["b", "c"], 2)
    Test.expect(obj.b.c).toBe(2)
end function

Test.test "list.sort2", function ()
    data = [3, 1, 2]
    sortFn = function (a, b)
        return a - b
    end function
    data = data.sort2(@sortFn)
    Test.expect(data[0]).toBe(1)
    Test.expect(data[2]).toBe(3)
end function

Test.runTests()
